{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "no-shadowed-variable": false
  },
  "rulesDirectory": [],
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:write"
    }
  },
  "linterOptions": {
    "exclude": ["**/migrations/**"]
  }
}
